{
  "name": "nekhbet/laravel-sphinxsearch",
  "description": "Laravel package to query SphinxSearch",
  "keywords": [
    "sphinx",
    "sphinxsearch",
    "laravel"
  ],
  "homepage": "https://github.com/nekhbet/laravel-sphinxsearch",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "sngrl",
      "email": "reserved@mail.ru"
    },
    {
      "name": "Sorin",
      "email": "trimbitassorin@hotmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Nekhbet\\SphinxSearch\\SphinxSearchServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*",
    "ext-bcmath": "*",
    "illuminate/support": "4.2.*|5.*|^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-0": {
      "Nekhbet\\SphinxSearch\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
    }
  }
}
